<p id="notice"><%= notice %></p>

<h1>Providers</h1>

<table>
  <thead>
    <tr>
      <th>Version</th>
      <th>Name</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @providers.each do |provider| %>
      <tr>
        <td><%= provider.version.version %></td>
        <td><%= provider.name %></td>
        <td><%= provider.url %></td>
        <td><%= link_to 'Show', user_box_version_provider_path(provider.params) %></td>
        <td><%= link_to 'Edit', edit_user_box_version_provider_path(provider.params) %></td>
        <td><%= link_to 'Destroy', user_box_version_provider_path(provider.params), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Provider', new_user_box_version_provider_path %>
