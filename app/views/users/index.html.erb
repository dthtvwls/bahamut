<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, user_boxes_path(user) %></td>
        <td><%= link_to '📝', edit_user_path(user) %></td>
        <td><%= link_to '🚮', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
